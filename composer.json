{
    "name": "nody/nody-blog",
    "description": "A Nody package wich add blog module to Nody's boilerplate",
    "keywords": [
        "nody",
        "laravel",
        "nody-blog"
    ],
    "homepage": "https://github.com/Thomas-DL/nody-blog",
    "support": {
        "issues": "https://github.com/Thomas-DL/nody-blog/issues",
        "source": "https://github.com/Thomas-DL/nody-blog"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nody",
            "email": "software@thomasdelage.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1"
    },
    "autoload": {
        "psr-4": {
            "Nody\\NodyBlog\\": "src/",
            "Nody\\NodyBlog\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nody\\NodyBlog\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nody\\NodyBlog\\NodyBlogServiceProvider"
            ],
            "aliases": {
                "NodyBlog": "Nody\\NodyBlog\\Facades\\NodyBlog"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
